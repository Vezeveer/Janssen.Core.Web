<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SAJCTI</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    @*<link rel="stylesheet" href="~/css/site.css" />*@

    <!-- Custom fonts for this template -->
    <link href="~/lib/fontawesome-free/css/all.min.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="css/grayscale.css" rel="stylesheet">
</head>

<body id="page-top">

    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
        <div class="container">
            @* LOGO BELOW *@
            <a class="navbar-brand js-scroll-trigger" href="#page-top"><img src="~/imgs/janssen_seal_logo.png" style="height: 60px !important;"/>  SAJCTI</a>
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                Menu
                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#about">About</a>
                    </li>
                    @*<li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#projects">Projects</a>
                    </li>*@
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#signup">Contact</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <!-- Header -->
    <header class="masthead">
        <div class="container d-flex h-100 align-items-center">
            <div class="mx-auto text-center">
                <h1 class="mx-auto my-auto text-uppercase main-heading">Saint Arnold Janssen College of Tacloban, Inc.</h1>
                <h2 class="text-white-50 mx-auto mt-2 mb-5">Creating Professionals</h2>
                <a href="#about" class="btn btn-primary js-scroll-trigger">Enroll</a>
                <a href="~/pdfs/ads_brochure01.pdf" class="btn btn-primary js-scroll-trigger" target="_blank">Brochure</a>
            </div>
        </div>
    </header>

    <div class="container sect-container">
        <div class="sect-content-container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

    <!-- About Section -->
    <section id="about" class="about-section text-center">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto">
                    <h2 class="text-white mb-4">About</h2>
                    <p class="text-white-50">
                        [Under Construction] Random words here but will add something later on.
                        <a href="#">something here</a>. More info coming soon.
                    </p>
                    <h2 class="text-white mb-4">Mission</h2>
                    <p class="text-white-50">
                        [Under Construction] Random words here but will add something later on.
                        <a href="#">something here</a>. More info coming soon.
                    </p>
                    <h2 class="text-white mb-4">Vision</h2>
                    <p class="text-white-50">
                        [Under Construction] Random words here but will add something later on.
                        <a href="#">something here</a>. More info coming soon.
                    </p>
                    <h2 class="text-white mb-4">Core Values</h2>
                    <p class="text-white-50">
                        [Under Construction] Random words here but will add something later on.
                        <a href="#">something here</a>. More info coming soon.
                    </p>
                </div>
            </div>
            <img src="~/imgs/janssen_seal_logo_large.png" class="img-fluid" alt="">
            <br />
            <br />
            <br />
            <br />
            <br />

        </div>
    </section>

    <!-- Projects Section -->
    <section id="projects" class="projects-section bg-light">
        <div class="container">

            <!-- Featured Project Row -->
            <div class="row align-items-center no-gutters mb-4 mb-lg-5">
                <div class="col-xl-8 col-lg-7">
                    <img class="img-fluid mb-3 mb-lg-0" src="~/imgs/placeholder.jpg" alt="">
                </div>
                <div class="col-xl-4 col-lg-5">
                    <div class="featured-text text-center text-lg-left">
                        <h4>Swimmin Pool Auditorium</h4>
                        <p class="text-black-50 mb-0">More info coming Soon...</p>
                    </div>
                </div>
            </div>

            <!-- Project One Row -->
            <div class="row justify-content-center no-gutters mb-5 mb-lg-0">
                <div class="col-lg-6">
                    <img class="img-fluid" src="~/imgs/placeholder.jpg" alt="">
                </div>
                <div class="col-lg-6">
                    <div class="bg-black text-center h-100 project">
                        <div class="d-flex h-100">
                            <div class="project-text w-100 my-auto text-center text-lg-left">
                                <h4 class="text-white">Crime Room</h4>
                                <p class="mb-0 text-white-50">More info coming soon...</p>
                                <hr class="d-none d-lg-block mb-0 ml-0">
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Project Two Row -->
            <div class="row justify-content-center no-gutters">
                <div class="col-lg-6">
                    <img class="img-fluid" src="~/imgs/placeholder.jpg" alt="">
                </div>
                <div class="col-lg-6 order-lg-first">
                    <div class="bg-black text-center h-100 project">
                        <div class="d-flex h-100">
                            <div class="project-text w-100 my-auto text-center text-lg-right">
                                <h4 class="text-white">Computer Lab</h4>
                                <p class="mb-0 text-white-50">This is our computer lab. More info coming soon...</p>
                                <hr class="d-none d-lg-block mb-0 mr-0">
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>

    <!-- Signup Section -->
    <section id="signup" class="signup-section">
        <div class="container">
            <div class="row">
                <div class="col-md-10 col-lg-8 mx-auto text-center">

                    <i class="far fa-paper-plane fa-2x mb-2 text-white"></i>
                    <h2 class="text-white mb-5">So what are you waiting for? Visit us or enroll now. Scroll down to find our contact info.</h2>

                    @*<form class="form-inline d-flex">
                        <input type="email" class="form-control flex-fill mr-0 mr-sm-2 mb-3 mb-sm-0" id="inputEmail" placeholder="Enter email address...">
                        <button type="submit" class="btn btn-primary mx-auto">Subscribe</button>
                    </form>*@

                    <div class="btn btn-primary mx-auto">Enroll</div>

                </div>
            </div>
        </div>
    </section>

    <!-- Contact Section -->
    <section class="contact-section bg-black">
        <div class="container">

            <div class="row">

                <div class="col-md-4 mb-3 mb-md-0">
                    <div class="card py-4 h-100">
                        <div class="card-body text-center">
                            <i class="fas fa-map-marked-alt text-primary mb-2"></i>
                            <h4 class="text-uppercase m-0">Address</h4>
                            <hr class="my-4">
                            <div class="small text-black-50">Janssen Bldg., LVD Compound, <br />Brgy. 45, Sta. Cruz Street, <br />6500 Tacloban City</div>
                        </div>
                    </div>
                </div>

                <div class="col-md-4 mb-3 mb-md-0">
                    <div class="card py-4 h-100">
                        <div class="card-body text-center">
                            <i class="fas fa-envelope text-primary mb-2"></i>
                            <h4 class="text-uppercase m-0">Email</h4>
                            <hr class="my-4">
                            <div class="small text-black-50">
                                <a href="#">coming soon</a>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-4 mb-3 mb-md-0">
                    <div class="card py-4 h-100">
                        <div class="card-body text-center">
                            <i class="fas fa-mobile-alt text-primary mb-2"></i>
                            <h4 class="text-uppercase m-0">Phone</h4>
                            <hr class="my-4">
                            <div class="small text-black-50">09156518049 Globe</div>
                            <div class="small text-black-50">09235231128 Smart</div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="social d-flex justify-content-center">
                @*<a href="#" class="mx-2">
                    <i class="fab fa-twitter"></i>
                </a>*@
                <a href="https://web.facebook.com/Saint-Arnold-Janssen-College-of-Tacloban-Inc-106138877763100/" class="mx-2">
                    <i class="fab fa-facebook-f"></i>
                </a>
                @*<a href="#" class="mx-2">
                    <i class="fab fa-github"></i>
                </a>*@
            </div>

        </div>
    </section>

    <!-- Footer -->
    <footer class="bg-black small text-center text-white-50">
        <div class="container">
            Copyright &copy; SAJCTI College of Criminal Education 2020
        </div>
    </footer>

